@model IEnumerable<Klmsncamp.Models.ParameterSetting>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Description
        </th>
        <th>
            ParameterValue
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ParameterValue)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ParameterSettingID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ParameterSettingID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ParameterSettingID })
        </td>
    </tr>
}

</table>
